                    <% var extConRune = extraContainer.split(' ').shift() %>
                    <% newRuneDescFinal = newRuneDescFinal.replace(runePowerBase(extConRune), (Math.round(runePowerRaw(extConRune) * 100) / 100) + runeIntPowerList[zz.toString()]['thirdInt']) %>
                    <% } %>

                    <% var id = newRuneDescFinal.replace(/[0-9.]/g, '') %>
                    <li><a class="rune-list" id="<%- id %>"><%- newRuneDescFinal %></a></li>
