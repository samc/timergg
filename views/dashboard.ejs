                    <% } %>

                    <% function runePowerBase(rune){ %>
                    <% if(rune.indexOf('+') != -1 || rune.indexOf('-') != -1){ %>
                    <% rune = rune.replace(/\-/g, '') %>
                    <% return parseFloat(rune) %>
                    <% } else return false; %>
                    <% } %>

                    <% var runeList = [] %>

                    <% for(var zz = 0; zz < allyTeam[z]['runeIds'].length; zz++){ %>

                    <% var runeIntPowerList = {} %>
                    <% var runeId = allyTeam[z]['runeIds'][zz]['runeId'] %>
                    <% var runeCount = allyTeam[z]['runeIds'][zz]['count'] %>
                    <% var runeDescSplit = runes[runeId]['description'].split(' / ') %>
                    <% var firstRuneInt = '' %>
                    <% var newRuneDescFinal = '' %>
                    <% var runeDesc = runes[runeId]['description'] %>
                    <% var runeItemInstance = runeDesc.replace(/[0-9.]/g, '') %>
                    <% runeList.push(runeDesc) %>

                    <%# add check if rune instance already exists and populate accordingly %>
                    <% var firstRunePower = 0 %>
                    <% var secondRunePower = 0 %>
                    <% var scalingRuneRef = 0 %>
                    <% for(var n = 0; n < runeList.length; n++) { %>
                    <% var runeInt = runeList[n] %>
                    <% var runeIntToBase = runeInt.replace(/[0-9.]/g, '') %>

                    <% if(runeList.indexOf(runeDesc) != n && runeItemInstance == runeIntToBase) { %>
                    <% var runePowerSplit = runeInt.split(' / ') %>
                    <% firstRunePower = firstRunePower + Math.round(runePowerBase(runePowerSplit[0].split(' ').shift()) * allyTeam[z]['runeIds'][n]['count']) %>
                    <% if (runePowerSplit.length == 2) secondRunePower = secondRunePower + Math.round(runePowerBase(runePowerSplit[1].split(' ').shift()) * allyTeam[z]['runeIds'][n]['count']) %>
                    <% if (hasContainer(runeDesc)){ %>
                    <% var ec = runeDesc.match(regContainer)[1] %>
                    <% scalingRuneRef = scalingRuneRef + Math.round(runePowerBase(ec.split(' ').shift()) * allyTeam[z]['runeIds'][n]['count']) %>
                    <% } %>
                    <% } %>
                    <% } %>

                    <% runeIntPowerList[zz.toString()] = ({
                        firstInt: firstRunePower,
                        secondInt: secondRunePower,
                        thirdInt: scalingRuneRef
                    }) %>

                    <% if(runeDescSplit.length == 1) { %>
                    <% firstRuneInt = runeDescSplit[0].split(' ').shift() %>
                    <% newRuneDescFinal = runeDesc.replace(runePowerBase(firstRuneInt), (Math.round(runePowerRaw(firstRuneInt) * 100) / 100) + runeIntPowerList[zz.toString()]['firstInt']) %>
                    <% } else { %>
                    <% firstRuneInt = runeDescSplit[0].split(' ').shift() %>
                    <% var secondRuneInt = runeDescSplit[1].split(' ').shift() %>
                    <% var newRuneDesc = runeDesc.replace(runePowerBase(firstRuneInt), (Math.round(runePowerRaw(firstRuneInt) * 100) / 100) + runeIntPowerList[zz.toString()]['firstInt']) %>
                    <% newRuneDescFinal = newRuneDesc.replace(runePowerBase(secondRuneInt), (Math.round(runePowerRaw(secondRuneInt) * 100) / 100) + runeIntPowerList[zz.toString()]['secondInt']) %>
                    <% } %>

                    <% if(hasContainer(newRuneDescFinal)) { %>
